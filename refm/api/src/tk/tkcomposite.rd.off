  require "tk"

  class TkLabeledEntry < TkEntry
    include TkComposite

    def initialize_composite(*args)
      @label = TkLabel.new(@frame)        # @frame、マエ茱劵栢促隋▲、Ε法◆◆
      @entry = TkEntry.new(@frame)
      @label.pack('side'=>'left')
      @entry.pack('side'=>'left')
      @path = @entry.path                 # TkEntry、ホ・皈ス・テ・ノ、好爛札、ケ、Ε宗

      delegate('DEFAULT', @entry, @label)
      delegate('text', @label)
    end
    def textvariable(var)
      @label.textvariable var
    end
  end

  TkLabeledEntry.new {
    text "foobar"
    self.value = "hoge"
    pack
  }

  Tk.mainloop

